        <!--This plugin's configuration is used to store Eclipse
                    m2e settings only. It has no influence on the Maven build itself. -->
        <plugin>
          <groupId>org.eclipse.m2e</groupId>
          <artifactId>lifecycle-mapping</artifactId>
          <version>1.0.0</version>
          <configuration>
            <lifecycleMappingMetadata>
              <pluginExecutions>
                <pluginExecution>
                  <pluginExecutionFilter>
                    <groupId>
                                            org.apache.maven.plugins
                                        </groupId>
                    <artifactId>
                                            maven-dependency-plugin
                                        </artifactId>
                    <versionRange>
                                            [2.1,)
                                        </versionRange>
                    <goals>
                      <goal>
                                                copy-dependencies
                                            </goal>
                    </goals>
                  </pluginExecutionFilter>
                  <action>
                    <ignore/>
                  </action>
                </pluginExecution>
                <pluginExecution>
                  <pluginExecutionFilter>
                    <groupId>
                                            org.apache.avro
                                        </groupId>
                    <artifactId>
                                            avro-maven-plugin
                                        </artifactId>
                    <versionRange>
                                            [1.7.5,)
                                        </versionRange>
                    <goals>
                      <goal>schema</goal>
                    </goals>
                  </pluginExecutionFilter>
                  <action>
                    <ignore/>
                  </action>
                </pluginExecution>
                <pluginExecution>
                  <pluginExecutionFilter>
                    <groupId>
                                            org.apache.thrift.tools
                                        </groupId>
                    <artifactId>
                                            maven-thrift-plugin
                                        </artifactId>
                    <versionRange>
                                            [0.1.10,)
                                        </versionRange>
                    <goals>
                      <goal>compile</goal>
                      <goal>testCompile</goal>
                    </goals>
                  </pluginExecutionFilter>
                  <action>
                    <ignore/>
                  </action>
                </pluginExecution>
                <pluginExecution>
                  <pluginExecutionFilter>
                    <groupId>
                                            org.apache.maven.plugins
                                        </groupId>
                    <artifactId>
                                            maven-antrun-plugin
                                        </artifactId>
                    <versionRange>
                                            [1.3,)
                                        </versionRange>
                    <goals>
                      <goal>run</goal>
                    </goals>
                  </pluginExecutionFilter>
                  <action>
                    <ignore/>
                  </action>
                </pluginExecution>
              </pluginExecutions>
            </lifecycleMappingMetadata>
          </configuration>
        </plugin>
        <plugin>
          <groupId>org.codehaus.mojo</groupId>
          <artifactId>build-helper-maven-plugin</artifactId>
          <version>1.9.1</version>
        </plugin>
      </plugins>
    </pluginManagement>
  </build>
  <profiles>
    <profile>
      <id>build-rpm</id>
      <build>
        <plugins>
          <plugin>
            <groupId>org.apache.maven.plugins</groupId>
            <artifactId>maven-dependency-plugin</artifactId>
            <version>2.8</version>
            <executions>
              <execution>
                <id>copy-rpm</id>
                <phase>compile</phase>
                <goals>
                  <goal>copy</goal>
                </goals>
                <configuration>
                  <artifactItems>
                    <artifactItem>
                      <groupId>org.kaaproject.kaa.sandbox-frame</groupId>
                      <artifactId>kaa-sandbox</artifactId>
                      <version>${kaa-sandbox-frame.version}</version>
                      <classifier>rpm</classifier>
                      <type>rpm</type>
                      <overWrite>true</overWrite>
                      <outputDirectory>${distro.path}</outputDirectory>
                      <destFileName>kaa-sandbox.rpm</destFileName>
                    </artifactItem>
                    <artifactItem>
                      <groupId>org.kaaproject.kaa.server</groupId>
                      <artifactId>kaa-node</artifactId>
                      <version>${kaa.version}</version>
                      <classifier>rpm</classifier>
                      <type>rpm</type>
                      <overWrite>true</overWrite>
                      <outputDirectory>${distro.path}</outputDirectory>
                      <destFileName>kaa-node.rpm</destFileName>
                    </artifactItem>
                  </artifactItems>
                </configuration>
              </execution>
            </executions>
          </plugin>
        </plugins>
      </build>
    </profile>
    <profile>
      <id>build-sandbox</id>
      <build>
        <plugins>
          <plugin>
            <groupId>org.codehaus.mojo</groupId>
            <artifactId>exec-maven-plugin</artifactId>
            <version>1.2.1</version>
            <executions>
              <execution>
                <phase>install</phase>
                <goals>
                  <goal>java</goal>
                </goals>
              </execution>
            </executions>
            <configuration>
              <mainClass>org.kaaproject.kaa.sandbox.SandboxMain</mainClass>
              <arguments>
                <argument>${base.path}</argument>
                <argument>vbox</argument>
                <argument>debian</argument>
                <argument>${ubuntu.image.url}</argument>
                <argument>${ubuntu.image.name}</argument>
                <argument>${ubuntu.sandbox.file}</argument>
                <argument>${ssh.forward.port}</argument>
                <argument>${web.admin.forward.port}</argument>
                <argument>${web.sandbox.forward.port}</argument>
                <argument>${web.zeppelin.forward.port}</argument>
                <argument>${bootstrap.tcp.forward.port}</argument>
                <argument>${bootstrap.http.forward.port}</argument>
                <argument>${operations.tcp.forward.port}</argument>
                <argument>${operations.http.forward.port}</argument>
                <argument>${analytics.enabled}</argument>
                <argument>${analytics.tracking.id}</argument>
              </arguments>
            </configuration>
          </plugin>
        </plugins>
      </build>
    </profile>
    <profile>
      <id>sandbox-cloud</id>
      <build>
        <plugins>
          <plugin>
            <groupId>org.codehaus.mojo</groupId>
            <artifactId>exec-maven-plugin</artifactId>
            <version>1.2.1</version>
            <executions>
              <execution>
